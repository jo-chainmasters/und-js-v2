{
  "name": "@unification-com/und-js-v2",
  "version": "0.1.4",
  "description": "Unification Mainchain Wallet SDK v2",
  "main": "lib/index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "example": "node --es-module-specifier-resolution=node example/fund.js",
    "build": "npx rimraf lib && npx babel src -d lib",
    "build:docs": "npx jsdoc-to-markdown -f src/**/*.js > docs/jsdoc.md",
    "prepublishOnly": "npm run build"
  },
  "author": "Codegnosis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unification-com/und-js-v2.git"
  },
  "keywords": [
    "FUND",
    "Unification"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/unification-com/und-js-v2/issues"
  },
  "homepage": "https://github.com/unification-com/und-js-v2",
  "dependencies": {
    "ansi-regex": "^5.0.1",
    "bech32": "^1.1.3",
    "big.js": "^6.1.1",
    "bip32": "^2.0.5",
    "bip39": "^2.5.0",
    "bitcoinjs-lib": "^4.0.2",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.1.1",
    "elliptic": "^6.5.4",
    "google-protobuf": "^3.14.0",
    "is_js": "^0.9.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "protobufjs": "^6.10.2",
    "request": "^2.88.2",
    "secp256k1": "^3.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.14.0",
    "@babel/helper-module-transforms": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.18.3",
    "@babel/types": "^7.14.1",
    "babel-eslint": "^10.1.0",
    "babelify": "^10.0.0",
    "browser-resolve": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "esmify": "^2.1.1",
    "jsdoc-to-markdown": "^7.1.1",
    "mocha": "^7.2.0",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "files": [
    "lib/*"
  ]
}
